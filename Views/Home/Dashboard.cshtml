                            <div class="text-center">
                                <h3>Your Tickets</h3>
                            </div>
                            <table id="myTTable" class="table table-hover">
                                <caption>Tickets for @btUser.FullName</caption>
                                <thead>
                                    <tr>
                                        <th>
                                            Title
                                        </th>
                                        <th>
                                            Priority
                                        </th>

                                        <th>
                                            Project
                                        </th>
                                        <th>
                                            Type
                                        </th>
                                        <th>
                                            Status
                                        </th>
                                        <th>
                                            Submitter
                                        </th>
                                        <th>
                                            Developer
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in (Model.Tickets).Where(t => t.OwnerUserId == btUser.Id || t.DeveloperUserId == btUser.Id))
